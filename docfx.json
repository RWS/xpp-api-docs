{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "docs/*.json",
          "docs/*.html",
          "docs/*.js",
          "docs/*.css"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "filemetadata": {
      "langs": {
        "api/**.yml": [ "csharp", "vb", "fsharp", "cpp" ]
      }
    },
    "template": [
      "RWSTemplate"
    ],
    "postProcessors": [ "ExtractSearchIndex", "EnvironmentVariableProcessor" ],
    "globalMetadata": {
      "_enableSearch": true
    },
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}